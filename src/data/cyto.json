{
  "boxSelectionEnabled": true,
  "userZoomingEnabled": true,
  "userPanningEnabled": true,
  "wheelSensitivity": 0.35,
  "layout": {
    "name": "cola",
    "infinite": false,
    "fit": true,
    "padding": 100,
    "nodeDimensionsIncludeLabels": false
  },
  "style": [
    {
      "selector": "*",
      "css": {
        "font-family": "Lato",
        "font-size": "12pt"
      }
    },
    {
      "selector": "node",
      "css": {
        "color": "#fefefe",
        "shape": "ellipse",
        "border-color": "#DAE4EA",
        "border-width": 2,
        "width": "100",
        "height": "100",
        "padding": 15,
        "text-wrap": "wrap",
        "text-max-width": 90,
        "text-halign": "center",
        "text-valign": "center"
      }
    },
    {
      "selector": ".faded",
      "css": {
        "background-opacity": 0.4
      }
    },
    {
      "selector": "node.eh-handle",
      "css": {
        "width": 10,
        "height": 10
      }
    },
    {
      "selector": "node.eh-hover",
      "css": {
        "border-width": 5
      }
    },
    {
      "selector": "node.segment",
      "css": {
        "background-color": "#fefefe",
        "border-color": "#DAE4EA",
        "color": "#28404D",
        "shape": "round-rectangle",
        "label": "data(label)",
        "width": "label",
        "height": "label",
        "padding": 20,
        "text-wrap": "wrap",
        "text-max-width": "4em",
        "text-halign": "center",
        "text-valign": "center"
      }
    },
    {
      "selector": "node.defaultLabel",
      "css": {
        "label": "data(label)"
      }
    },
    {
      "selector": "node.assumption",
      "css": {
        "background-color": "#F3BE8C"
      }
    },
    {
      "selector": "node.hypothesis",
      "css": {
        "background-color": "#F2B6B5"
      }
    },
    {
      "selector": "node.experiment",
      "css": {
        "background-color": "#AADEF6"
      }
    },
    {
      "selector": "node.learning",
      "css": {
        "background-color": "#B0E5DD"
      }
    },
    {
      "selector": "node.task",
      "css": {
        "background-color": "#B8B5F7"
      }
    },
    {
      "selector": "edge",
      "css": {
        "line-color": "#888"
      }
    }
  ],
  "elements": {
    "nodes": [],
    "edges": []
  }
}
